etIdTokenResult();\r\n        // redux store\r\n        console.log(\"user\", user, \"tokenResult\", tokenResult);\r\n\r\n        addUpdateUser(tokenResult.token)\r\n          .then((res) => {\r\n            console.log(\"ADD-UPDATE RESPONSE\", res);\r\n\r\n            dispatch({\r\n              type: \"LOGGED_IN_USER\",\r\n              payload: {\r\n                name: res.data.name,\r\n                email: res.data.email,\r\n                token: tokenResult.token,\r\n                role: res.data.role,\r\n                _id: res.data._id,\r\n              },\r\n            });\r\n          })\r\n          .catch((error) => console.log(error));\r\n\r\n        // redirect\r\n        history.push(\"/\");\r\n      }\r\n    } catch (error) {\r\n      console.log(error);\r\n      toast.error(error.message);\r\n    }\r\n  };\r\n  return (\r\n    <div className=\"container p-5\">\r\n      <div className=\"row\">\r\n        <div className=\"col-md-6 offset-md-3\">\r\n          <ToastContainer />\r\n          <h4>Complete Registration</h4>\r\n          <form onSubmit={handleSubmit}>\r\n            <input\r\n              type=\"email\"\r\n              className=\"form-control mb-3\"\r\n              value={email}\r\n              disabled\r\n            />\r\n\r\n            <input\r\n              type=\"password\"\r\n              className=\"form-control\"\r\n              value={password}\r\n              onChange={(e) => setPassword(e.target.value)}\r\n              placeholder=\"Enter Your Password\"\r\n              autoFocus\r\n            />\r\n\r\n            <button type=\"submit\" className=\"btn btn-raised btn-block mt-3\">\r\n              Complete Registration\r\n            </button>\r\n          </form>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default RegisterCompletePage;\r\n","C:\\Users\\41762\\Desktop\\udemyFS\\ecommerce\\frontend\\src\\store.js",[],"C:\\Users\\41762\\Desktop\\udemyFS\\ecommerce\\frontend\\sr