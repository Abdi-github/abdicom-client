ing(false);\r\n            toast.error(err.response.data);\r\n          }\r\n        });\r\n    }\r\n  };\r\n\r\n  const searched = (keyword) => (c) => c.name.toLowerCase().includes(keyword);\r\n\r\n  return (\r\n    <div className=\"container-fluid\">\r\n      <div className=\"row\">\r\n        <div className=\"col-md-2\">\r\n          <AdminNav />\r\n        </div>\r\n        <div className=\"col\">\r\n          <ToastContainer />\r\n          {loading ? <Loading /> : <h4>Create category</h4>}\r\n\r\n          <CategoryInputForm\r\n            name={name}\r\n            setName={setName}\r\n            handleSubmit={handleSubmit}\r\n          />\r\n\r\n          <SearchInput keyword={keyword} setKeyword={setKeyword} />\r\n\r\n          <hr />\r\n          {/* {JSON.stringify(categories)} */}\r\n\r\n          {categories.filter(searched(keyword)).map((c) => (\r\n            <div className=\"alert alert-secondary\" key={c._id}>\r\n              {c.name}\r\n              <span\r\n                onClick={() => deleteHandler(c.slug)}\r\n                className=\"btn btn-md float-right\"\r\n              >\r\n                <DeleteOutlined className=\"text-danger\" />\r\n              </span>\r\n              <Link to={`/admin/category/${c.slug}`}>\r\n                <span className=\"btn btn-md float-right\">\r\n                  <EditOutlined className=\"text-warning\" />\r\n                </span>\r\n              </Link>\r\n            </div>\r\n          ))}\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default CategoryCreate;\r\n","C:\\Users\\41762\\Desktop\\udemyFS\\ecommerce\\frontend\\src\\functions\\category.js",[],"C:\\Users\\41762\\Desktop\\udemyFS\\ecommerce\\frontend\\src\\pages\\admin\\category\\CategoryUpdatePage.js",["903"],"import React, { useState, useEffect } from \"react\";\r\nimport AdminNav from \"../../../components/nav/AdminNav\";\r\nimport { toast, ToastContainer } from \"react-toastify\";\r\nimport { useSelect