tion>Please select</option>\r\n                {categories.length > 0 &&\r\n                  categories.map((c) => (\r\n                    <option key={c._id} value={c._id}>\r\n                      {c.name}\r\n                    </option>\r\n                  ))}\r\n              </select>\r\n            </div>\r\n\r\n            {/* {subCategoryOptions ? subCategoryOptions.length : \"no subs\"} */}\r\n            {showSubCategories && (\r\n              <div>\r\n                <label>Sub Categories</label>\r\n                <Select\r\n                  mode=\"multiple\"\r\n                  style={{ width: \"100%\" }}\r\n                  placeholder=\"Please select\"\r\n                  value={subCategories}\r\n                  onChange={(value) =>\r\n                    setValues({ ...values, subCategories: value })\r\n                  }\r\n                >\r\n                  {subCategoryOptions.length &&\r\n                    subCategoryOptions.map((s) => (\r\n                      <Option key={s._id} value={s._id}>\r\n                        {s.name}\r\n                      </Option>\r\n                    ))}\r\n                </Select>\r\n              </div>\r\n            )}\r\n            <button className=\"btn btn-outline-info btn-block\">Save</button>\r\n          </form>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default ProductCreatePage;\r\n","C:\\Users\\41762\\Desktop\\udemyFS\\ecommerce\\frontend\\src\\functions\\product.js",[],"C:\\Users\\41762\\Desktop\\udemyFS\\ecommerce\\frontend\\src\\components\\Input-forms\\FileUpload.js",[],"C:\\Users\\41762\\Desktop\\udemyFS\\ecommerce\\frontend\\src\\components\\cards\\AdminProductCard.js",[],"C:\\Users\\41762\\Desktop\\udemyFS\\ecommerce\\frontend\\src\\pages\\admin\\product\\AllProductsPage.js",[],"C:\\Users\\41762\\Desktop\\udemyFS\\ecommerce\\frontend\\src\\pages\\admin\\product\\ProductUpdatePage.js",["920","921"],"import React, { useEffect, useState } 